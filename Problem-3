import java.util.Scanner;

public class Problem-3 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter input: ");
		int a = sc.nextInt();
		a = a%2 ==0? a-1:a;
		a*=2;
		for(int i=1;i<=a;i+=2){
			if(i==a || i== a-1)
				System.out.print(i);
			else
				System.out.print(i + ",");
		}

	}
}


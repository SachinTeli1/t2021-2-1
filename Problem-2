import java.util.Scanner;

public class Problem-2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter input: ");
		int a = sc.nextInt();

		for(int i =1;i<=2*a;i++) {
			if(i%2==1) {
				if(i>1) {
					System.out.print(",");
				}
				System.out.print(i);
			}
		}
	}
}

